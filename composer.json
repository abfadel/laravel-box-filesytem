{
    "name": "abfadel/laravel-box-api-adapter",
    "description": "Laravel filesystem adapter for Box.com API with JWT authentication",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "box",
        "box.com",
        "filesystem",
        "storage",
        "jwt"
    ],
    "authors": [
        {
            "name": "abfadel"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^9.0|^10.0|^11.0",
        "league/flysystem": "^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Abfadel\\BoxAdapter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abfadel\\BoxAdapter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abfadel\\BoxAdapter\\BoxServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
